{
    "description": "This repository contains GroupDocs.Merger Cloud SDK for PHP source code.",
    "name": "groupdocscloud/groupdocs-merger-cloud",
    "version": "23.10",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "groupdocs",
        "merger",
        "cloud",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/groupdocs-merger-cloud/groupdocs-merger-cloud-php",
    "authors": [
        {
            "name": "GroupDocs",
            "homepage": "https://github.com/groupdocs-merger-cloud"
        }
    ],
    "config": {
       "platform": {
            "php": "8.1"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.5.8|^7.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0"
    },
    "autoload": {
        "classmap": [ "src/" ],
        "psr-4": { "GroupDocs\\Merger\\" : "src/" }
    },
    "autoload-dev": {
        "classmap": [ "src/", "tests/" ],
        "psr-4": { "GroupDocs\\Merger\\" : "tests/" }
    }
}